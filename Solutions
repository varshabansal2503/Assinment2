//ANS-2: MAX OF THREE
public class Solution {

    public static void main(String[] args) {
      
 
   int a=5;
        int b=6;
        int c=7;
        if(a>b && a>c)
        {
            System.out.println(a);
        }
        else if(b>a && b>c)
        {
            System.out.println(b);
        }
        else
        {
            System.out.println(c);
        }
}
