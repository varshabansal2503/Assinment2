// ANS -1 :MAX OF TWO
public class Solution {

    public static void main(String[] args) {
      
        int A=5;
        int B=6;
        if(A>B)
        {
            System.out.println(A);
        }
        else
        {
             System.out.println(B);
        }
    }
}
