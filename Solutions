//ANS-9: WHICH TRIANGLE---
  int x=5,y=6,z=7;
        
        if (x== y && y == z ) {
            System.out.println("Equilateral Triangle"); 
        }

        
        else if (x == y || y == z || z == x ) {
            System.out.println("Isoceles Triangle");
        } 

       
        else if (x != y || y!= z || z != x){
            System.out.println("scalene");
}
--------------------------------------------------------------------------------------------------------------------------------------
//ANS-10 CATEGORYY NUMBER--
 int A=50;
        if(A>0)
        {
            System.out.println("1");
        }
        else if(A<0)
        {
          System.out.println("-1");
        }
        else{
            System.out.println("0");
        }

